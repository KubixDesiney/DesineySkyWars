on rightclick with 95 named "&6&lPLATFORM MAGIC":
    if {skywars.kit.%player%} is "Sauveteur":
        set {schem} to player's location
        paste schematic "platform.schematic" at {schem}
        wait 1 tick
        paste schematic "platform1.schematic" at {schem}
        wait 1 tick
        paste schematic "platform2.schematic" at {schem}
        send "&6KIT &avous avez 10 seconds pour s'enfuir avant que la platform disparaît"
        wait 11 seconds
        paste schematic "air.schematic" at {schem}
on place:
    if {skywars.isStart} is set:
        if event-block is 166 named "&f&lShield":
            cancel event
every 3 minutes:
        loop all players:
            if {skywars.isStart} is set:
                if {skywars.kit.%loop-player%} is "Teleporteur":
                    give loop-player 1 368
                    send "&6Kit &avous avez obtenu une enderpearl grace a votre kit !"
                    stop
on projectile hit:
    if projectile is an arrow:
        if {explode.%shooter%} is true:	
            create explosion of force 4 at projectile
            stop
on projectile hit:
    {arrows::%projectile%} is true
    delete {arrows::%projectile%}
    delete projectile
on shoot:
    name of shooter's tool is "&cArc &7- &6KA-BOOM"
    set {explode.%shooter%} to true
on shoot:
    if shooter is a player:
        if projectile is an arrow:
            set {arrows::%projectile%} to true
on join:
    if {skywars.kit.%player%} is "Archer":
        set {explode.%player%} to false
every 5 minutes:
    loop all players:
        if {skywars.isStart} is set:
            if {skywars.kit.%player%} is "Archer":
                give player 1 261 named "&cArc &7- &6KA-BOOM"
                give player 2 262
                send "&6Kit &avous avez obtenu l'arc d'explosion grace a votre kit !"
on death of a player:
	if {skywars.kit.%victim%} is "Kamikaze":
		create explosion of force 5 in radius 10 of victim
		play raw sound "mob.wither.spawn" at attacker with pitch 1 volume 100
		send "&6Kit &cVotre explosion a était fait avec &asuccés"


		
#---------------------------------------------------------------------------#
#					   		GESTION DE TIMING								#
#---------------------------------------------------------------------------#

every 1 second:
	if {skywars.isStart} is set:
		if {skywars.isPaused} is not set:
			add 1 to {skywars.Time.S}
			if {skywars.Time.S} is more than or equal to 60:
				set {skywars.Time.S} to 0
				add 1 to {skywars.Time.M}
	make all players execute command "/timermaj"
every 1 second:
    if {skywars.Time.M} is equal to 60:
        add 1 to {skywars.Time.H}
every 1 second:
    if {skywars.isStart} is set:
        if {skywars.Event} is "&aRemplissage des coffres:"
            remove 1 from {skywars.Event.S}
            if {skywars.Event.S} is equal to 0:
                remove 1 from {skywars.Event.M}
                wait 1 second
                set {skywars.Event.S} to "59"
                if {skywars.Event.M} is equal to 0:
                    execute console command "/firstload"
                    wait 1 second
                    execute console command "/refill"
#---------------------------------------------------------------------------#
#					   	 		SYSTEM DE Scoreboard						#
#---------------------------------------------------------------------------#
#Si vous voulez changer un ligne svp contacter moi car c un peu compliqué
command /timermaj:
	trigger:
		if {skywars.isStart} is not set:
			erase player's sidebar
			set name of sidebar of player to "&f&l» &b&lSkywars &f&l«"
			set score "&7Status: &fEn attente des joueurs" in sidebar of player to 8
			set score "" in sidebar of player to 7
			if {skywars.teams} is true:
				set score "&7Mode: &f%{skywars.teammax}%vs%{skywars.teammax}%" in sidebar of player to 6
			else:
				set score "&7Mode: &fSOLO &f- &cNoCrossTeam" in sidebar of player to 6
			if number of all players is equal to {skywars.minplayers}:
				set score "&7Joueurs: &a%number of all players%&8/&f24" in sidebar of player to 5
			else:
				set score "&7Joueurs: &f%number of all players%&8/&f24" in sidebar of player to 5
			set score "&7Aide: &f/skywars" in sidebar of player to 4
			set score "&7kit: &6%{skywars.kit.%player%}%" in sidebar of player to 3
			
		else:
			erase player's sidebar
			set name of sidebar of player to "&f&l» &b&lSkywars &f&l«"
			set score "%{skywars.Event}%" in sidebar of player to 10
			set score "&f%{skywars.Event.M}%:%{skywars.Event.S}" in sidebar of player to 9
			set score "&e&lDurée: &f%{skywars.Time.H}%&f:&f%{skywars.Time.M}%&f:&f%{skywars.Time.S}%" in sidebar of player to 8
			loop all players:
				set score "&7kills: &f%{skywars.ingame.kills.%player%}%" in sidebar of player to 7
			set score "" in sidebar of player to 6
			if {skywars.teams} is true:
				set score "&7Mode: &f%{skywars.teammax}%vs%{skywars.teammax}%" in sidebar of player to 5
			else:
				set score "&6Mode: &fFFA - &cNoCrossTeam" in sidebar of player to 5
			set score "&7Joueurs en vie: &a%{skywars.players}%" in sidebar of player to 4
			if {Equipe::%player%} is "Blue":
				set score "&7Team: &9Blue" in sidebar of player to 3
			if {Equipe::%player%} is "Red":
				set score "&7Team: &cRouge" in sidebar of player to 3
			if {Equipe::%player%} is "Magenta":
				set score "&7Team: &5Magenta" in sidebar of player to 3
			if {Equipe::%player%} is "Yellow":
				set score "&7Team: &eJaune" in sidebar of player to 3
			if {Equipe::%player%} is "Green":
				set score "&7Team: &aVert" in sidebar of player to 3
			if {Equipe::%player%} is "Purple":
				set score "&7Team: &5Violet" in sidebar of player to 3
			if {Equipe::%player%} is "Cyan":
				set score "&7Team: &3Cyan" in sidebar of player to 3
			if {Equipe::%player%} is "Orange":
				set score "&7Team: &6Orange" in sidebar of player to 3
			if {Equipe::%player%} is "Pink":
				set score "&7Team: &dRose" in sidebar of player to 3
			if {Equipe::%player%} is "Aqua":
				set score "&7Team: &bBleue clair" in sidebar of player to 3
			if {Equipe::%player%} is "Black":
				set score "&7Team: &0Noir" in sidebar of player to 3
			if {Equipe::%player%} is "Gray":
				set score "&7Team: &7Gris" in sidebar of player to 3
