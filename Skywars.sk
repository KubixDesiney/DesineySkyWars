command /toconfig [<text>]:
    trigger:
        if player is op:
            if arg 1 is "on":
                set {skywars.teams} to true
                loop all players:
                    give loop-player 1 425 named "&6Choisi une team"
                broadcast ""
                broadcast "{@prefix}&7Les teams sont maintenant activer par &e&l%executor% !"
                broadcast ""
            if arg 1 is "off":
                set {skywars.teams} to false
                loop all players:
                    clear loop-player's inventory
                broadcast ""
                broadcast "{@prefix} &7Les teams sont maintenant desactiver par &e&l%executor% !"
                broadcast ""
                set {skywars.teammax} to 0
                set {skywars.Bluelimit} to 0
                set {skywars.Redlimit} to 0
                set {skywars.Greenlimit} to 0
                set {skywars.Magentalimit} to 0
                set {skywars.Purplelimit} to 0
                set {skywars.Orangelimit} to 0
                set {skywars.Yellowlimit} to 0
                set {skywars.Graylimit} to 0
                set {skywars.Cyanlimit} to 0
                set {skywars.Blacklimit} to 0
                set {skywars.Aqualimit} to 0
                set {skywars.Pinklimit} to 0
                execute console command "/scoreboard teams empty Blue"
                execute console command "/scoreboard teams empty Red"
                execute console command "/scoreboard teams empty Green"
                execute console command "/scoreboard teams empty Magenta"
                execute console command "/scoreboard teams empty Purple"
                execute console command "/scoreboard teams empty Orange"
                execute console command "/scoreboard teams empty Yellow"
                execute console command "/scoreboard teams empty Gray"
                execute console command "/scoreboard teams empty Cyan"
                execute console command "/scoreboard teams empty Black"
                execute console command "/scoreboard teams empty Aqua"
                execute console command "/scoreboard teams empty Pink"
on join:
    if {skywars.noconnect} is set:
        kick player due to "&eLa partie est en train de reset"
every 3 seconds:
    if {skywars.isStart} is not set:
        if number of all players >= {skywars.minplayer}:
            broadcast ""
            broadcast "{@prefix} &aLa partie va se lancer dans 30 secondes"
            broadcast ""
            stop
            if {skywars.teams} is true:
                broadcast ""
                broadcast "{@prefix} &r&7N'oubliez pas de choisir une team !"
                broadcast ""
                stop
            else:
                broadcast "{@prefix} &r&cLe crossteam et &c&l&nInterdit"
                broadcast ""
                stop
            wait 24 seconds
            broadcast "" 
            broadcast "{@prefix} &aLa partie va se lancer dans 5 secondes"
            broadcast ""	
            stop
            wait 6 seconds
            execute console command "/game forcestart"
    loop all players:
        set action bar of loop-player to "&cJoueurs manque &e%{skywars.minplayer} - number of all players%"
    if {skywars.isStart} is set:
        loop all players:
            set action bar of loop-player to " "
command /minplayers [<text>] [<number>]:
    trigger:
        if arg 1 is "set":
            if number argument is set:
                set {skywars.minplayers} to arg 2
command /bc [<text>]:
	trigger:
		if player is op:
			if arg 1 is set:
				broadcast ""
				broadcast "&7&l¦ &e&lBROADCAST &r&e%player% &f&l» &f%arg 1%"
				broadcast ""
			else:
				send "&cMerci de définir un message" 
command /maxteams [<text=help>] [<number>]:
    permission: admin.skywars
    trigger:
        if arg 1 is not set:
            send "&cUsage: /maxteams set <nomber>"
        if arg 1 is "set":
            if arg 2 is not set:
                send "&cUsage: /maxteams set <nombre>"
            if number argument is set:
                set {skywars.teammax} to arg 2
                broadcast "{@prefix} &r&7Le max des teams est maintenant &e%{skywars.teammax}% &7par &e&l%executor%"
