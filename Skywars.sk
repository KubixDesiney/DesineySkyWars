		send "&ale mid a été defeni"
command /stats [<player>]:
    trigger:
        if arg 1 is set:
            send "&b&m---------------------------------------------"
            send " "
            send " &6Les statistiques de %arg 1 %"
            send " "
            send "&bKills: &f%{skywars.kills.%arg 1%}%"
            send "&bWin: &f%{skywars.win.%arg 1 %}%"
            send "&bcoins: &f%{altasy.coins.%arg 1%}%"
            send "&bnombre de parties joués: &f%{skywars.games.%arg 1%}%"
            if {skywars.win.%arg 1%} is less than 1000:
                send "&bNiveau de joueur: &aPro Player"
            else if {skywars.win.%arg 1%} is less than 600:
                send "&bNiveau de joueur: &6Moyen"
            else if {skywars.%arg 1%} is less than 200:
                send "&bNiveau de joueur: &cNoob"
            send " "
            send "&6Les statistiques de %arg 1%"
            send " "
            send "&b&m---------------------------------------------"
