#---------------------------------------------------------------------------#
#					    	 SYSTEME DU MOTD								#
#---------------------------------------------------------------------------#
				
on server list ping:
	if {skywars.isStart} is not set: #Si le jeu n'est pas lancé
		set motd to "&f&l» &e&lSkywars &f&l« &7 En attente de joueurs .. "
	else:
		set motd to "&f&l» &e&lSkywars &f&l« &a Partie en cours ! "	
		
#---------------------------------------------------------------------------#
#					      	SYSTEME DE DE/CONNEXION							#
#---------------------------------------------------------------------------#

on join:
	if {skywars.isStart} is not set: #Si le jeu n'est pas lancé
		if {skywars.teams} is true:
			give player 1 425 named "&6Choix d'equipe &7(click droit)"
			broadcast "{@prefix} &7Le joueur &e%player% &7a rejoint la partie &8(&e%number of all players%&7/&e24&8)"
		else:
			broadcast "{@prefix} &7Le joueur &e%player% &7a rejoint la partie &8(&e%number of all players%&7/&e24&8)"
		teleport player to location (0, 200, 0) #TP à la WaitingRoom
		clear player's inventory
		set the gamemode of the player to adventure #Gamemode aventure au joueur
		heal player #Heal le joueur
		add 1 to {skywars.games.%player%}
		set tab header to "&b&lSkywars" and footer to "&8En attente de joueurs..." for player
	if {skywars.isStart} is set: #Si le jeu est lancé
		send "{@prefix} &7Le jeu est deja commence ! vous etes maintenant spectateur"
		execute console command "/gamemode 3 %player%"
		set {Equipe::%player%} to "spec"
		if {skywars.isPaused} is set:
			set the freeze state of player to true
		else:
			set the freeze state of player to false
			
on place:
	if {skywars.isStart} is not set:
		cancel event
	if {skywars.isPaused} is set:
		cancel event
		send "{@prefix} &cLa partie est en pause !"

on break:
	if {skywars.isStart} is not set:
		cancel event
	if {skywars.isPaused} is set:
		cancel event
		send "&cLa partie est en pause !"
		
		
on drop: #Evite le drop en lobby
	if {skywars.isStart} is not set:
		cancel event
	if {skywars.isPaused} is set:
		cancel event
		send "&cLa partie est en pause !"
			
On Hunger MEter Change: #Evite le changement de faim
	if {skywars.isStart} is not set:
		cancel event
	if {skywars.isPaused} is set:
		cancel event
			
			
On damage: #Evite le changement de vie
	if {skywars.isStart} is not set: #Si le jeu n'est pas lancé
		cancel event
	if {skywars.isPaused} is set:
		cancel event
		
on damage:
	if {skywars.Damage} is not set:
		if victim is a player:
			cancel event
on damage:
    if damage cause is fall:
        if {skywars.nofall} is true:
            cancel event
