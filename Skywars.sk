on chat:
	cancel event
	if {Equipe::%player%} is not set:
		broadcast "&7%player% &f» %message%"
	else:
		if first character of message is "!":
			replace "!" with "" in the message
			if first character of message is set:
				if {Equipe::%player%} is "Blue":
					broadcast "&f[&eGlobal&f] &9%player% &f» %message%"
				if {Equipe::%player%} is "Red":
					broadcast "&f[&eGlobal&f] &c%player% &f» %message%"
				if {Equipe::%player%} is "Green":
					broadcast "&f[&eGlobal&f] &a%player% &f» %message%"
				if {Equipe::%player%} is "Magenta":
					broadcast "&f[&eGlobal&f] &d%player% &f» %message%"
				if {Equipe::%player%} is "Purple":
					broadcast "&f[&eGlobal&f] &5%player% &f» %message%"
				if {Equipe::%player%} is "Orange":
					broadcast "&f[&eGlobal&f] &6%player% &f» %message%"
				if {Equipe::%player%} is "Yellow":
					broadcast "&f[&eGlobal&f] &e%player% &f» %message%"
				if {Equipe::%player%} is "Gray":
					broadcast "&f[&eGlobal&f] &7%player% &f» %message%"
				if {Equipe::%player%} is "Cyan":
					broadcast "&f[&eGlobal&f] &3%player% &f» %message%"
				if {Equipe::%player%} is "Black":
					broadcast "&f[&eGlobal&f] &0%player% &f» %message%"
				if {Equipe::%player%} is "Aqua":
					broadcast "&f[&eGlobal&f] &b%player% &f» %message%"
				if {Equipe::%player%} is "Pink":
					broadcast "&f[&eGlobal&f] &d%player% &f» %message%"
		else:
			loop all players:
				if {Equipe::%player%} is "Blue":
					if {Equipe::%loop-player%} is "Blue":
						send "&f(Équipe) &9%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Red":
					if {Equipe::%loop-player%} is "Red":
						send "&f(Équipe) &c%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Green":
					if {Equipe::%loop-player%} is "Green":
						send "&f(Équipe) &a%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Magenta":
					if {Equipe::%loop-player%} is "Magenta":
						send "&f(Équipe) &d%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Purple":
					if {Equipe::%loop-player%} is "Purple":
						send "&f(Équipe) &5%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Orange":
					if {Equipe::%loop-player%} is "Orange":
						send "&f(Équipe) &6%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Yellow":
					if {Equipe::%loop-player%} is "Yellow":
						send "&f(Équipe) &e%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Gray":
					if {Equipe::%loop-player%} is "Gray":
						send "&f(Équipe) &7%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Cyan":
					if {Equipe::%loop-player%} is "Cyan":
						send "&f(Équipe) &3%player% &f» %message% " to loop-player
				if {Equipe::%player%} is "Black":
					if {Equipe::%loop-player%} is "Black":
						send "&f(Équipe) &0%player% &f» %message%" to loop-player
				if {Equipe::%player%} is "Aqua":
					if {Equipe::%loop-player%} is "Aqua":
						send "&f(Équipe) &b%player% &f» %message%)" to loop-player
				if {Equipe::%player%} is "Pink":
