#---------------------------------------------------------------------------#
command /skywars [<text=help>] [<text>] [<player>]:
    permission: admin.skywars
    trigger:			
        if arg 1 is "start":
            if {skywars.isStart} is not set:
                loop all players:
                    set the freeze state of loop-player to true
                    set the loop-player's gamemode to survival
                    heal loop-player
                    clear inventory of loop-player
                    set slot 0 of loop-player to 12 cooked beef
                    if {Equipe::%loop-player%} is not set
                        add loop-player to {noteam::*}
                        if {skywars.bluelimit} is 0:
                            if {skywars.teamamx} is 3:
                                set size of {noteam::*} to 3
                                make loop-player execute command "/team blue"
                            if {skywars.teamamx} is 2:
                                set size of {noteam::*} to 2
                                make loop-player execute command "/team blue"
                        if {skywars.bluelimit} is 1:
                            if {skywars.teamamx} is 3:
                                set size of {noteam::*} to 2
                                make loop-player execute command "/team blue"
                            if {skywars.teamamx} is 2:
                                set size of {noteam::*} to 1
                                make loop-player execute command "/team blue"
                        if {skywars.bluelimit} is 2:
                            if {skywars.teamamx} is 3:
                                set size of {noteam::*} to 1
                                make loop-player execute command "/team blue"
                    execute console command "/start1"
                    wait 2 seconds
                    if {skywars.teams} is false:
                        if {skywars.Players} is 1:
                            teleport player to {skywars.SoloSpawn.1}
                        if {skywars.Players} is 2:
                            teleport player to {skywars.SoloSpawn.2}
                        if {skywars.Players} is 3:
                            teleport player to {skywars.SoloSpawn.3}
                        if {skywars.Players} is 4:
                            teleport player to {skywars.SoloSpawn.4}
                        if {skywars.Players} is 5:
                            teleport player to {skywars.SoloSpawn.5}
                        if {skywars.Players} is 6:
                            teleport player to {skywars.SoloSpawn.6}
                        if {skywars.Players} is 7:
                            teleport player to {skywars.SoloSpawn.7}
                        if {skywars.Players} is 8:
                            teleport player to {skywars.SoloSpawn.8}
                    else:
                        if {Equipe::%loop-player%} is "Blue":
                            teleport loop-player to {skywars.TeamSpawn.1}
                        if {Equipe::%loop-player%} is "Red":
                            teleport loop-player to {skywars.TeamSpawn.2}
                        if {Equipe::%loop-player%} is "Green":
                            teleport loop-player to {skywars.TeamSpawn.3}
                        if {Equipe::%loop-player%} is "Magenta":
                            teleport loop-player to {skywars.TeamSpawn.4}
                        if {Equipe::%loop-player%} is "Purple":
                            teleport loop-player to {skywars.TeamSpawn.5}
                        if {Equipe::%loop-player%} is "Orange":
                            teleport loop-player to {skywars.TeamSpawn.6}
                        if {Equipe::%loop-player%} is "Yellow":
                            teleport loop-player to {skywars.TeamSpawn.7}
                        if {Equipe::%loop-player%} is "Gray":
                            teleport loop-player to {skywars.TeamSpawn.8}
                        if {Equipe::%loop-player%} is "Cyan":
                            teleport loop-player to {skywars.TeamSpawn.9}
                        if {Equipe::%loop-player%} is "Black":
                            teleport loop-player to {skywars.TeamSpawn.10}
                        if {Equipe::%loop-player%} is "Aqua":
                            teleport loop-player to {skywars.TeamSpawn.11}
                        if {Equipe::%loop-player%} is "Pink":
                            teleport loop-player to {skywars.TeamSpawn.12}
            else:
                send "{@prefix} &r&7La partie est déja en cours ! Merci d'avoir utiliser &e/game reset"
        if arg 1 is "spawn":
            if arg 2 is not set:
                send "&e-------------- &fAide: &bSkyWars&e--------------"
                send "&7Il faut définir les 12 spawns des équipes. (1;2;3.. 12)"
            else:
                set {skywars.TeamSpawn.%arg 2%} to player's location
                send "&aSpawn n&e%arg 2% definis &e%{skywars.TeamSpawn.%arg 2%}%"
        if arg 1 is "spawnsolo":
            if arg 2 is not set:
                send "&6&m-----------------[Aide Skywars]---------------------------"
                send "&7Il faut definir les 8 spawn (1,2,3,4,5 etc...)"
            else:
                set {skywars.SoloSpawn.%arg 2%} to player's location
                send "&aSpawn N &e%arg 2% &aa ete definis: &e%{skywars.SoloSpawn.%arg 2%}%"

        if arg 1 is "reset":
            loop all players:
                teleport loop-player to location (0, 200, 0)
                set loop-player's tab list name to "&f%loop-player%"
            delete {skywars.isStart}
            clear {Equipe::*}
            delete {skywars.isPaused}
            set {skywars.players} to 0
            delete {skywars.EventAction}
            delete {skywars.Event}
            delete {skywars.Event.S}
            delete {skywars.noconnect}
            delete {skywars.Damage}
            broadcast ""
            broadcast "{@prefix} &r&7La partie a été reset par &e&l%executor%"
            broadcast ""
            set {skywars.bluelimit} to 0
            set {skywars.redlimit} to 0
            set {skywars.Greenlimit} to 0
            set {skywars.Magentalimit} to 0
            set {skywars.Purplelimit} to 0
            set {skywars.Orangelimit} to 0
            set {skywars.Yellowlimit} to 0
            set {skywars.Graylimit} to 0
            set {skywars.Cyanlimit} to 0
            set {skywars.Blacklimit} to 0
            set {skywars.Aqualimit} to 0
            set {skywars.Pinklimit} to 0
            execute console command "/scoreboard teams empty Blue"
            execute console command "/scoreboard teams empty Red"
            execute console command "/scoreboard teams empty Green"
            execute console command "/scoreboard teams empty Magenta"
            execute console command "/scoreboard teams empty Purple"
            execute console command "/scoreboard teams empty Orange"
            execute console command "/scoreboard teams empty Yellow"
            execute console command "/scoreboard teams empty Gray"
            execute console command "/scoreboard teams empty Cyan"
            execute console command "/scoreboard teams empty Aqua"
            execute console command "/scoreboard teams empty Black"
            execute console command "/scoreboard teams empty Pink"
